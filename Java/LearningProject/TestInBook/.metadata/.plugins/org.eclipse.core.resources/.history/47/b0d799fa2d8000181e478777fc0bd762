package javaFoundation;

public class GodenFenge {
	public static void main(String[]args) {
		double d=21,c=0;int a=0,b=0;
		for(int i=1;i<21;i++) {
			for(int j=1;j<21;j++) {
				c=Math.abs(i/j-0.618);
				System.out.println(c);
				if(true){
					d=c;a=i;b=j;
					System.out.println(a+" "+b+" "+c);
				}
			}
		}
		System.out.println(a+" "+b);
	}
}

