package javaFoundation;

public class shuiXianhua {
	public static void main(String[]args) {
		for(long i=1;i<10;i++) {
			for(long j=1;j<10;j++) {
				for(long k=1;k<10;k++) {
					if(i*i*i+j*j*j+k*k*k==100*i+10*j+k) {
						System.out.println("水仙花数："+i+""+j+""+k);
					}
				}
			}
		}
	}
	public static void main(String[]args) {
		for(long i=1;i<8;i++) {
			for(long j=1;j<8;j++) {
				for(long k=1;k<10;k++) {
					for(long l=1;l<14;l++) {
						if(i+j==8 && i+k==14 && k-l==6 && l+j==10) {
							System.out.println("答案："+i+" "+j+" "+k+" "+l);
						}
					}
				}
			}
		}
	}
}
