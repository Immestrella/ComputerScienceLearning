package javaFoundation;

public class XiaoxueMath {
	public static void main(String[]args) {
		int i=0,j=0,k=0,l=0;
		for(i=1;i<80;i++) {
			for(j=1;j<80;j++) {
				for(k=1;k<100;k++) {
					for(l=1;l<140;l++) {
						if(i+j==8 && i+k==14 && k-l==6 && l+j==10) {
							System.out.println("´ð°¸£º"+i+" "+j+" "+k+" "+l);
						}
					}
				}
			}
		}
	}
}
