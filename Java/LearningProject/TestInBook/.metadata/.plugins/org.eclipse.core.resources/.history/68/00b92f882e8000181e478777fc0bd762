package javaFoundation;

public class GodenFenge {
	public static void main(String[]args) {
		double d=21,c=0,e=0;int a=0,b=0;
		for(int i=1;i<21;i++) {
			for(int j=1;j<21;j++) {
				e=i/j-0.618;
				c=Math.abs(e);
				System.out.println(e+" "+c);
				if((i%2!=0 && j%2!=0) && c<d){
					d=c;a=i;b=j;
					System.out.println(a+" "+b+" "+c);
				}
			}
		}
		System.out.println(a+" "+b);
	}
}

