package javaFoundation;

public class XiaoxueMath {
	public static void main(String[]args) {
		for(int i=1;i<8;i++) {
			for(int j=1;j<8;j++) {
				for(int k=1;k<14;k++) {
					for(int l=1;l<10;l++) {
						if(i+j==8 && i+k==14 && k-l==6 && l+j==10) {
							System.out.println("´ð°¸£º"+i+" "+j+" "+k+" "+l);
						}
					}
				}
			}
		}
	}
}
